<form method="post" action="{{ path('rubrique_delete', {'idRubrique': rubrique.idRubrique}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rubrique.idRubrique) }}">
    <button class="btn">Delete</button>
</form>
